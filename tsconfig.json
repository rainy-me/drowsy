{
    "compilerOptions": {
      "module": "commonjs",
      "lib": [
        "es2017",
        "es2016",
        "es2015",
        "dom"
      ],
      "outDir": "dist/",
      "noImplicitAny": true,
      "allowJs": false,
      "removeComments": true
    },
    "include": [
      "src",
      "node_modules/@types/puppeteer/index.d.ts"
    ],
    "rules": {
      "no-console": false,
      "interface-name": [
        true,
        "never"
      ]
    }
  }
  